#include <bits/stdc++.h> 
int findJudge(int n, int q, vector<vector<int> > &trust){
	// Write your code here.
	vector<int>indegree(n+1,0);
	vector<int>outDegree(n+1,0);
	for(auto edge : trust){
		int u=edge[0];
		int v=edge[1];
		indegree[v]++;
		outDegree[u]++;
	}
	int Judge=-1;
	for(int i=1;i<=n;i++){
		if(indegree[i]==n-1 && outDegree[i]==0){
			Judge=i;
		}
	}
	return Judge;
}